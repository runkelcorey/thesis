  month = jun,
  year = {2018},
  pages = {233-245},
  file = {C\:\\Users\\Corey\\Zotero\\storage\\C4ZKXHSS\\Bloch et al. - 2018 - Beyond bus fare Deconstructing Prenatal Care Trav.pdf},
  pmid = {29969975},
  pmcid = {PMC6310900}
}

