  journal = {Journal of the Academy of Nutrition and Dietetics},
  doi = {10.1016/j.jand.2018.04.017},
  author = {Ma, Xiaonan and Sharpe, Patricia A. and Bell, Bethany A. and Liu, Jihong and White, Kellee and Liese, Angela D.},
  month = oct,
  year = {2018},
  keywords = {Transportation,Community food resources,Grocery store type,Shopping distance,Shopping frequency},
  pages = {1844-1854},
  file = {C\:\\Users\\Corey\\Zotero\\storage\\8H2WU2K7\\Ma et al. - 2018 - Food Acquisition and Shopping Patterns among Resid.pdf;C\:\\Users\\Corey\\Zotero\\storage\\QI4FS62E\\S2212267218306518.html}
}

