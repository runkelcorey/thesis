@article{ambrosiusBlueCityRed2016,
  title = {Blue {{City}} \ldots{} {{Red City}}? {{A Comparison}} of {{Competing Theories}} of {{Core County Outcomes}} in {{U}}.{{S}}. {{Presidential Elections}}, 2000\textendash{}2012},
  shorttitle = {Blue {{City}}... {{Red City}}?},
