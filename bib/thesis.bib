@book{newmanHomeFrontImplications1999,
  title = {The {{Home Front}}: {{Implications}} of {{Welfare Reform}} for {{Housing Policy}}},
  shorttitle = {The {{Home Front}}},
  author = {Newman, Sandra J.},
  year = {1999},
